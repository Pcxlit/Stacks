#include <iostream>
#include <stack>
#include <string>
using namespace std;
bool isvalid(string str)
{
    stack<char> s;
    for (int i = 0; i < str.size(); i++)
    {
        if (str[i] =='(' || str[i] =='{' || str[i] == '[')
        {

            s.push(str[i]);
        }
        else
        {
            if (s.size() == 0)
            {
                return false;
            }
            if ((s.top() == '(' && str[i] == ')') || (s.top() == '{' && str[i] == '}') || (s.top() == '[' && str[i] == ']'))
            {
                s.pop();
            }
            else
            {
                return false;
            }
        }
    }
    if (s.size()==0){
        return true;
    }
    else
    {
        return false;
    }
    
}
int main()
{
   cout<<boolalpha<<isvalid("({{[]]}}");
    return 0;
}
